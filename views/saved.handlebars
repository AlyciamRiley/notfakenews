{{!-- Inserting of the header --}}
	{{> header }}
{{!-- End of the header insert --}}  
  
  <div class="container">
    <div id="articles">
      {{#each headline}}
      <div class="card {{this.id}}" style="width: 50rem;">
        <div class="card-body pink lighten-5" id="card">

          <h5 class="card-title brown darken-1 ">{{this.title}}

          </h5>

          <a href="{{this.link}}" class="card-link">{{this.link}}</a>
        </div>
      </div>
      {{/each}}
    </div>
  </div>